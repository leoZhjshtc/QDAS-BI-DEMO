        <!-- <if test="teilNum !='' and teilNum!=null and teilNum !='undefined' and teilNum !='null'">
             and t.TETEILNR = #{teilNum}
         </if>
         <if test="teilNum =='' or teilNum==null or teilNum =='undefined' or teilNum =='null'">
