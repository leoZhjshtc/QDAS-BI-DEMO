        <if test="teilK1003 !='' and teilK1003!=null and teilK1003 !='undefined' and teilK1003 !='null'">
            and t.TEKURZBEZEICH = #{teilK1003}
        </if>
        <if test="teilK1003 =='' or teilK1003==null or teilK1003 =='teilK1003' or teilK1003 =='null'">
